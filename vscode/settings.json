{
    "breadcrumbs.enabled": true,
    "center-editor-window.threeStateToggle": true,
    "csharp.referencesCodeLens.enabled": false,
    "crystal-lang.completion": true,
    "crystal-lang.hover": true,
    "crystal-lang.implementations": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.tokenColorCustomizations": {
        "[One Monokai]": {
            "comments": "#676f7d"
        }
    },
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "Cargo.lock": "toml",
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitProjectManager.baseProjectsFolders": [
        "/home/damien/src",
        "/home/damien/Graphics"
    ],
    "gitProjectManager.openInNewWindow": true,
    "gitProjectManager.recentProjectsListSize": 10,
    "gitProjectManager.searchInsideProjects": false,
    "GodotTools.completeNodePath": true,
    "julia.enableTelemetry": false,
    "markdown.preview.breaks": true,
    "python.analysis.openFilesOnly": false,
    "python.autoComplete.addBrackets": true,
    "python.jediEnabled": false,
    "python.linting.flake8Args": ["--ignore=E701,E271"],
    "python.linting.flake8CategorySeverity.F": "Warning",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "todohighlight.defaultStyle": {
        "color": "lightblue",
        "backgroundColor": "#00000000",
    },
    "todohighlight.exclude": [
        "**/target/**",
        "**/.stack-work/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**"
    ],
    "todohighlight.include": [
        "**/*.rs",
        "**/*.hs",
        "**/*.toml"
    ],
    "todohighlight.keywords": [
        {
            "text": "BUG",
            "color": "#ff4d4d",
        },
        "CHANGED",
        "COMBAK",
        {
            "text": "DEBUG",
            "color": "#b38324",
        },
        {
            "text": "FIXME",
            "color": "#ffbb33",
        },
        "HACK",
        "IDEA",
        {
            "text": "NB",
            "color": "#3399ff",
        },
        {
            "text": "NOTE",
            "color": "#3399ff",
        },
        "OPTIMIZE",
        "QUESTION",
        "REVIEW",
        "TEMP",
        {
            "text": "TODO",
            "color": "#24b36b",
        },
        {
            "text": "WARNING",
            "color": "#ff8133",
        },
        "XXX",
    ],
    "window.menuBarVisibility": "visible",
    "window.openFilesInNewWindow": "default",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "One Monokai",
    "workbench.commandPalette.preserveInput": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.feedback.visible": false,
}
